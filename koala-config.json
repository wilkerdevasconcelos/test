{
  "mappings": [
    {
      "src": "assets/scss",
      "dest": "assets/css"
    }
  ],
  "ignores": ["*.css"],

  "options": {
    "outputStyle": "compressed",
    "lineComments": false,
    "debugInfo": false,
    "sourceMap": false,

    "unixNewlines": false,
    "autoprefix": false,
    "autoprefixConfig": "> 1%, last 2 versions, Firefox ESR, Opera 12.1"
  },

  "customOptions": [],
  "includePaths": [],

  "requireLibs": []
}
